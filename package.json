  "license": "MIT",
  "dependencies": {
    "chalk": "2.3.2",
    "commander": "2.14.1",
    "ejs": "2.5.7",
    "lodash.zipwith": "^4.2.0",
    "mkdirp": "^0.5.1"
  },
  "devDependencies": {
    "husky": "0.15.0-rc.13",
    "snazzy": "7.1.1",
    "standard": "11.0.1"
  }
}
